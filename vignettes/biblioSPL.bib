@ARTICLE{136,
  author = {Boyd, I. L. and McCann, T. S.},
  title = {Pre-natal investment in reproduction by female Antarctic fur seals},
  journal = {Behav Ecol Sociobiol},
  year = {1989},
  volume = {24},
  pages = {377-385},
  abstract = {Maternal investment, in terms of pup birth mass, in gestation by Antarctic
	fur seals (\textit{Arctocephalus gazella}) was related to the date
	of birth in two consecutive years. There were significant differences
	in birth mass between years and between sexes within years. Birth
	mass was used to calculate the mean energetic cost of producing a
	pup to parturition. The cost for male pup was always higher than
	the cost for a female pup. Given the probable pattern of foetal growth,
	this constitutes a minimum of 5-15\% of the maternal energy budget
	in the final months of gestation. Birth mass varied inversely with
	date of birth, but more strongly for male than for female pups. The
	sex ratio was unity in both years and it did not vary with time through
	the birth season. In a subsample, there was a decline in the body
	mass and standard length with date of birth. Male birth masswas directly
	related to maternal mass and condition (mass/length) but there was
	no significant relationship for females. These results suggest that
	the growth of male foetuses is limited by maternal resources while
	female foetuses do not exploit fully maternal resources.},
  file = {Boyd&McCann89.pdf:Boyd&McCann89.pdf:PDF},
  key = {136},
  keywords = {Antarctic fur seals, \textit{Arctocephalus gazella}, reproduction,
	female investment in reproduction},
