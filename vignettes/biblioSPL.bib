@ARTICLE{6096,
  author = {Y. Frenot and E. de Oliveira and M. Gauthier-Clerc and J. Deunff
	and A. Bellido and P. Vernon},
  title = {{L}ife cycle of the tick \textit{Ixodes uriae} in penguin colonies:
